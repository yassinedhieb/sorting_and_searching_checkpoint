ALGORITHM algorithm_name
VAR
    arr : ARRAY_OF INTEGER[100];
    j,i,key:INTEGER;
BEGIN
    FOR i FROM 0 TO arr.length-1 DO
        key=arr[i];
        j:=i-1;
        WHILE (j>=0 && key<arr[j]) DO
            arr[j+1]:=arr[j]
            j:=j-1;
        END_WHILE
        arr[j+1]:=key;
    END_FOR
    FOR i FROM 0 TO arr.length-1 DO
        write(arr[i]);
    END_FOR
END